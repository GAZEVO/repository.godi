<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="plugin.video.godi" name="godi entertainment" version="0.0.1" provider-name="gazevo">
	<requires>
		<import addon="xbmc.python" version="2.19.0" />
		<import addon="script.module.nanscrapers" version="0.5.5" />
        <import addon="script.module.python.koding.aio" version="0.0.1"/>
	</requires>
	<extension point="xbmc.python.pluginsource" library="default.py">
		<provides>audio video</provides>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary lang="en">video, music and TV shows</summary>
		<description lang="en">Full one stop shop entertainment addon.</description>
		<platform>all</platform>
		<forum>http://www.noobsandnerds.com/support</forum>
		<website>www.noobsandnerds.com</website>
		<news></news>
		<genre>kids,livetv,movies,music,tvshows,arabic,australian,czech,danish,dutch,egyptian,filipino,french,german,greek,hebrew,icelandic,indian,irish,japanese,Korean,newzealand,polish,portuguese,spanish,swedish,uk,usa</genre>
		<video_guide></video_guide>
		<video_preview></video_preview>
	</extension>
</addon>

<addon id="repository.godi" name="godi" version="0.0.1" provider-name="gazog">
